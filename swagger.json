                    "name":{"type":"string"},
                    "type":{"type":"string"},
                    "imageURL":{"type":"string"}
                }
                }
            }
        ],
        "responses": {
          "201": {
            "description": "Successfully Create Grab Restaurant",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "number"
                    },
                    "name": {
                      "type": "string"
                    },
                    "type": {
                      "type": "string"
                    },
                    "imageURL": {
                      "type": "string"
                    },
                    "createdAt": {
                      "type": "string"
                    },
                    "updatedAt": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Failed to Create Restaurant"
          }
        }
      }  
    },
