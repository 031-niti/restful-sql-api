{
  "swagger": "2.0",
  "info": {
    "title": "Grab Restaurant API",
    "description": "All ReSTful API Endpoint for Grab Restaurant",
    "version": "0.0.1"
  },
  "paths": {
    "/api/auth/signin": {
      "post": {
        "tags": ["SignIn & SignUp"],
        "description": "Sign in to Grab Restaurant",
        "parameters":[{
            "name":"body",
            "in":"body",
            "required":"body",
            "schema":{
                "type":"object",
                "properties":{
                    "username":{"type":"string"},
                    "password":{"type":"string"}
                }
                }
            }
        ],
        "responses": {
          "200": {
            "description": "Successfully Sign in ",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "number"
                    },
                    "username": {
                      "type": "string"
                    },
                    "email": {
                      "type": "string"
                    },
                    "roles": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      }
                    },
                    "accessToken": {
                      "type": "string"
                    }
                  },
                    "example": {
                        "id": 1,
                        "username": "username",
                        "email": "email",
                        "roles": [
                            "ROLES_USER"
                        ],
                        "accessToken": "eyJhbGci...lJmKyeECo"
                    }
                }
              }
            }
          },
          "401": {
            "description": "User not found!"
          },
          "404": {
            "description": "Invalid Password"
          }
        }
      }
    },
    "/api/auth/signup": {
      "post": {
        "tags": ["SignIn & SignUp"],
        "description": "Sign up to Grab Restaurant",
        "parameters":[{
            "name":"body",
            "in":"body",
            "required":"body",
            "schema":{
                "type":"object",
                "properties":{
                    "username":{"type":"string"},
                    "email":{"type":"string"},
                    "password":{"type":"string"},
                    "roles":{"type":"string"}
                }
                }
            }
        ],
        "responses": {
          "200": {
            "description": "Successfully Sign up ",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "username": {
                      "type": "string"
                    },
                    "email": {
                      "type": "string"
                    },
                    "password": {
                      "type": "string"
                    },
                    "roles": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
